baseURL: https://example.com
title: Your Name
languageCode: en-us
theme: charlolamode
pagination:
  pagerSize: 8

menu:
  main:
    - name: Projects
      url: /projects/
      weight: 10

params:
  author: Your Name

  profile:
    name: Your Name
    bio: >-
      Short punchy bio about your role and focus.
    image: /images/profile.png

  socials:
    - name: github
      url: https://github.com/your-handle
    - name: linkedin
      url: https://linkedin.com/in/your-handle
    - name: stackoverflow
      url: https://stackoverflow.com/users/your-id
    - name: buymeacoffee
      url: https://buymeacoffee.com/your-handle

  projects:
    heading: Selected Projects
    emptyState: No projects published yet.

  assets:
    favicon: /images/favicon.ico
    favicon16x16: /images/favicon-16x16.png
    favicon32x32: /images/favicon-32x32.png
    apple_touch_icon: /images/apple-touch-icon.png
    safari_pinned_tab: /images/apple-touch-icon.png

enableInlineShortcodes: true
enableRobotsTXT: true
enableEmoji: true
pygmentsUseClasses: true

minify:
  disableXML: true